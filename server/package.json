{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist",
    "start:dev": "nodemon --config nodemon.json",
    "start:prod": "NODE_ENV=production pm2 start --name server ts-node -- -r tsconfig-paths/register ./src/index.ts ",
    "test": "nodemon --config nodemon.test.json",
    "generate": "graphql-codegen"
  },
  "_moduleAliases": {
    "@service": "dist/service",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/start",
    "@util": "dist/util"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.6.0",
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-resolvers": "^1.12.2",
    "apollo-server": "^2.10.0",
    "apollo-server-express": "^2.10.0",
    "assert": "^2.0.0",
    "command-line-args": "^5.1.1",
    "consolidate": "^0.15.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "elasticsearch": "^16.6.0",
    "express": "~4.16.1",
    "express-session": "^1.16.2",
    "graphql": "^14.6.0",
    "graphql-type-json": "^0.3.1",
    "http-errors": "~1.6.3",
    "http-status-codes": "^1.4.0",
    "merge-graphql-schemas": "^1.7.6",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "mustache": "^4.0.0",
    "nodemon": "^1.19.2",
    "path": "^0.12.7",
    "pm2": "^3.5.1",
    "session": "^0.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/consolidate": "^0.14.0",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/find": "^0.2.1",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^3.5.3",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.1",
    "@types/supertest": "^2.0.8",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jest": "^3.5.0",
    "jsonfile": "^5.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
